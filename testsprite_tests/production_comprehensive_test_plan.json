  "tests": [
    {
      "id": "TC_PROD_001",
      "name": "Production Backend Health Check",
      "type": "api",
      "priority": "critical",
      "steps": [
        {
          "action": "GET",
          "url": "https://hma-saas-1.onrender.com/health",
          "expectedStatus": 200
        }
      ]
    },
    {
      "id": "TC_PROD_002",
      "name": "Production Frontend Homepage Load",
      "type": "frontend",
      "priority": "critical",
      "steps": [
        {
          "action": "navigate",
          "url": "https://hma-saas-web.vercel.app",
          "waitFor": "networkidle"
        }
      ]
    },
    {
      "id": "TC_PROD_003",
      "name": "Production Login Flow",
      "type": "e2e",
      "priority": "critical",
      "steps": [
        {
          "action": "navigate",
          "url": "https://hma-saas-web.vercel.app/login"
        },
        {
          "action": "fill",
          "selector": "input[name='email']",
          "value": "admin@test.com"
        },
        {
          "action": "fill",
          "selector": "input[name='password']",
          "value": "Admin@123"
        },
        {
          "action": "click",
          "selector": "button[type='submit']"
        },
        {
          "action": "waitForNavigation",
          "url": "/dashboard"
        }
      ]
    },
    {
      "id": "TC_PROD_004",
      "name": "Dashboard Load After Login",
      "type": "frontend",
      "priority": "critical",
      "steps": [
